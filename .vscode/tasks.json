{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build main (gcc)",
            "type": "shell",
            "command": "gcc",
            "args": [
                "-g",
                "${workspaceFolder}\\mna_solver\\main.c",
                "${workspaceFolder}\\mna_solver\\mna_solver.c",
                "${workspaceFolder}\\mna_solver\\mna_solver_initialize.c",
                "${workspaceFolder}\\mna_solver\\mna_solver_terminate.c",
                "${workspaceFolder}\\mna_solver\\rt_nonfinite.c",
                "${workspaceFolder}\\mna_solver\\rtGetInf.c",
                "${workspaceFolder}\\mna_solver\\rtGetNaN.c",
                "-I${workspaceFolder}\\mna_solver",
                "-I${workspaceFolder}\\include",
                "-o",
                "${workspaceFolder}\\mna_solver\\main.exe"
            ],
            "group": {
                "kind": "build",
                "isDefault": false
            },
            "problemMatcher": [
                "$gcc"
            ]
        },
        {
            "type": "cppbuild",
            "label": "C/C++: gcc.exe build active file",
            "command": "C:\\mingw64\\bin\\gcc.exe",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${file}",
                "-o",
                "${fileDirname}\\${fileBasenameNoExtension}.exe"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ]
}